{
  "name": "ngx-editor-vs",
  "version": "4.1.0",
  "description": "WYSIWYG Editor for Angular Applications",
  "license": "MIT",
  "scripts": {
    "build": "ng build --prod",
    "build:package": "sh ./scripts/build.sh",
    "docs": "compodoc && npm run publish:docs",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "lint:scripts": "eslint ./scripts",
    "ng": "ng",
    "postbuild": "npm run build:package",
    "prebuild": "npm run lint",
    "prepublishOnly": "node ./scripts/prepublish.js",
    "prerelease": "npm run build",
    "publish:docs": "node ./scripts/publishdocs.js",
    "release": "sh ./scripts/release.sh",
    "start": "ng serve --aot -o --port 5000 --host 0.0.0.0",
    "serve:docs": "compodoc --serve --open --output=\"docs/\"",
    "test": "ng test",
    "test:single-run": "ng test --watch false"
  },
  "keywords": [
    "angular-editor",
    "angular-wysiwyg-editor",
    "wysiwyg-editor",
    "ngx-editor",
    "angular2-wysiwyg-editor",
    "angular4-wysiwyg-editor"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sibiraj-s/ngx-editor.git"
  },
  "bugs": {
    "url": "https://github.com/sibiraj-s/ngx-editor/issues"
  },
  "homepage": "https://github.com/sibiraj-s/ngx-editor",
  "peerDependencies": {
    "font-awesome": "^4.7.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.24",
    "@angular/animations": "^8.2.14",
    "@angular/cli": "^8.3.24",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@compodoc/compodoc": "^1.1.11",
    "@types/jasmine": "^3.5.3",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "~13.7.0",
    "bootstrap": "^4.4.1",
    "chalk": "^3.0.0",
    "codelyzer": "^5.0.1",
    "core-js": "^3.6.4",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    "font-awesome": "^4.7.0",
    "gh-pages": "^2.2.0",
    "husky": "^4.2.1",
    "inquirer": "^7.0.4",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "~3.1.0",
    "karma-jasmine-html-reporter": "^1.5.2",
    "ng-packagr": "^5.4.0",
    "ngx-bootstrap": "^5.3.2",
    "protractor": "^5.4.3",
    "rxjs": "~6.5.4",
    "ts-node": "~8.6.2",
    "tslint": "^6.0.0",
    "typescript": "^3.4.5",
    "zone.js": "~0.9.1"
  },
  "dependencies": {
    "tslib": "^1.10.0"
  }
}
